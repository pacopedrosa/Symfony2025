{% extends 'base.html.twig' %}

{% block title %}RegistroSalida index{% endblock %}

{% block body %}
    <h1>RegistroSalida index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Cantidad</th>
                <th>Fecha_salida</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for registro_salida in registro_salidas %}
            <tr>
                <td>{{ registro_salida.id }}</td>
                <td>{{ registro_salida.cantidad }}</td>
                <td>{{ registro_salida.fechaSalida ? registro_salida.fechaSalida|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_registro_salida_show', {'id': registro_salida.id}) }}">show</a>
                    <a href="{{ path('app_registro_salida_edit', {'id': registro_salida.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_registro_salida_new') }}">Create new</a>
{% endblock %}

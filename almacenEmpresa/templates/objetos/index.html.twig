{% extends 'base.html.twig' %}

{% block title %}Objetos index{% endblock %}

{% block body %}
    <h1>Objetos index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Descripcion</th>
                <th>Cantidad</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for objeto in objetos %}
            <tr>
                <td>{{ objeto.id }}</td>
                <td>{{ objeto.descripcion }}</td>
                <td>{{ objeto.cantidad }}</td>
                <td>
                    <a href="{{ path('app_objetos_show', {'id': objeto.id}) }}">show</a>
                    <a href="{{ path('app_objetos_edit', {'id': objeto.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_objetos_new') }}">Create new</a>
{% endblock %}
